<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/style.css">
    <title>Create a Users</title>
</head>

<body>
    <div class="topnav">
        <a href="/">Home</a>
        <a href="/create-user" class="active">Create Users</a>
    </div>

    <div class="form-add-new">
        <form action="/create-user" method="post">
            <fieldset>
                <legend>Form add new users</legend>
                <div class="input-group">
                    <label>Email: </label>
                    <input type="text" name="email" />
                </div>
                <div class="input-group">
                    <label>Name: </label>
                    <input type="text" name="myName" />
                </div>
                <div class="input-group">
                    <label>City: </label>
                    <input type="text" name="myCity" />
                </div>
                <div>
                    <button>Save</button>
                </div>
            </fieldset>
        </form>
    </div>
</body>

</html>